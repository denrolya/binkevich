<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Home{% endblock title %} | Binkevich</title>

        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('build/common.css') }}" />
            <link rel="stylesheet" href="{{ asset('build/main.css') }}" />
        {% endblock stylesheets %}
        <link href="https://fonts.googleapis.com/css?family=Montserrat:300,400,500,600,700" rel="stylesheet">
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <meta name="viewport" content="width=device-width, initial-scale=1">
    </head>

    <body>
        <div id="content"></div>

        <script src="{{ asset('bundles/fosjsrouting/js/router.min.js') }}"></script>
        <script src="{{ path('fos_js_routing_js', { callback: 'fos.Router.setData' }) }}"></script>
        {% block javascripts %}
            <script type="text/javascript" src="{{ asset('build/common.js') }}"></script>
        {% endblock javascripts %}
    </body>

</html>
